def find_sum(numbers):
    # Инициализируем переменную для хранения суммы
    total = 0
    
    # Проходимся по каждому числу в списке
    for num in numbers:
        # Добавляем число к общей сумме
        total += num
    
    # Возвращаем общую сумму
    return total

# Заданный список чисел
my_numbers = [10, 20, 30, 40, 50]

# Вызываем функцию для нахождения суммы чисел в списке и выводим результат
print("Сумма чисел в списке:", find_sum(my_numbers))
